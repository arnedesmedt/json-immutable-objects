{
    "name": "arnedesmedt/json-immutable-objects",
    "type": "library",
    "description": "Immutable objects with a json schema",
    "homepage": "https://github.com/arnedesmedt/json-immutable-objects",
    "license": "MIT",
    "authors": [
        {
            "name": "Arne De Smedt",
            "email": "arnedesmdt@gmail.com",
            "homepage": "https://github.com/arnedesmedt"
        }
    ],
    "require": {
        "php": "^8",
        "arnedesmedt/value-objects": "~0.1",
        "event-engine/php-json-schema": "~2.0",
        "phpdocumentor/reflection-docblock": "~5.0",
        "symfony/yaml": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~9.0",
        "phpro/grumphp": "~1.0",
        "phpstan/phpstan": "~1.0",
        "squizlabs/php_codesniffer": "~3.0",
        "doctrine/coding-standard": "~11.0",
        "rector/rector": "~0.1",
        "phpmd/phpmd": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "ADS\\JsonImmutableObjects\\": "src/",
            "ADS\\JsonImmutableObjects\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
